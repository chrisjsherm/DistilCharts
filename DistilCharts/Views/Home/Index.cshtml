<div class="row">
    <div class="small-12 columns">
        <div id="container"></div>
    </div>
</div>

<div class="row">
    <div class="small-12 columns">
        <form id="dynamicAdd">
            <input type="number" name="temp" required />
            <input type="text" name="month" required />
            
            <input type="radio" name="series" value="0" checked="checked" />Tokyo
            <input type="radio" name="series" value="1" />New York
            <input type="radio" name="series" value="2" />Berlin
            <input type="radio" name="series" value="3" />London

            <button type="submit">
                Add
            </button>
        </form>
    </div>
</div>