@using System.Web.Optimization

<!doctype html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@System.Configuration.ConfigurationManager.AppSettings["AppName"]</title>
    <meta name="description" content="" />
    <meta name="application-name" content="@System.Configuration.ConfigurationManager.AppSettings["AppName"]">

    <link href="http://fonts.googleapis.com/css?family=Raleway:600,400,200" rel="stylesheet" type="text/css">

    @Styles.Render("~/css/styles")
</head>

<body>
    @RenderBody()

    @Scripts.Render("~/scripts/vendor")
    @RenderSection("scripts", required: false)
</body>
</html>
